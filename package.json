    "mapper",
    "mapping",
    "json2typescript",
    "jsonconvert",
    "class",
    "object",
    "instance"
  ],
  "author": "dhlab@unibas.ch",
  "license": "MIT",
  "homepage": "http://dhlab.unibas.ch",
